  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Calculate term frequencies according to Xiao et al.(2020)\n",
    "rel_term_freq = clean_corpus['TF']/len(clean_corpus)\n",
    "inverse_freq = 1/rel_term_freq\n",
    "term_xiao = np.log2(inverse_freq.astype('float64'))\n",
    "TFX_df = clean_corpus.dropna()\n",
    "TFX_df['TFX'] = term_xiao\n",
    "# Save the term frequency DataFrame\n",
    "TFX_df.to_pickle('TFX_df.pkl')\n"
   ]
