    "\n",
    "# Filter all words with a lower frequency than freq_treshold\n",
    "# Entries were non-sensical words below 21\n",
    "freq_treshold = 21\n",
    "df_filtered = df_filtered[df_filtered['abs_freq']>=freq_treshold]\n",
    "# set all entries to lowercase\n",
    "df_filtered['lemma'] = df_filtered['lemma'].str.lower()\n",
    "\n",
    "df_filtered"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 15,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Lemmatize each entry\n",
    "# ~47 minutes\n",
    "\n",
    "# set to True if you already processed the courpus frequency file\n",
    "if False:\n",
    "    clean_corpus = pd.read_pickle(\"clean_corpus.pkl\")\n",
    "else:\n",
    "\n",
    "    lem_terms_list = []\n",
    "    old_df_terms = df_filtered['lemma']\n",
    "    new_df_terms = df_filtered[['lemma', 'abs_freq']]\n",
    "\n",
    "    # make each entry go through the nlp pipeline\n",
    "    for i in range(len(new_df_terms)-1):\n",
    "        term = str(old_df_terms.iloc[i])\n",
    "        pipe_term = nlp.pipe([term])\n",
    "\n",
    "        # lemmatize each entry\n",
    "        for doc in pipe_term:\n",
    "            for token in doc:\n",
    "                lemma_token = token.lemma_\n",
    "\n",
    "                if lemma_token != term:\n",
    "                    lem_terms_list.append([term,lemma_token])\n",
    "                    new_df_terms.iloc[i,0] = lemma_token\n",
    "\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 24,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Group now identical lemmatized entries and sum their frequencies\n",
    "preclean_corpus = new_df_terms.groupby('lemma')['abs_freq'].sum().to_frame().drop('', axis=0)\n",
    "# Set all words to lowercase\n",
    "preclean_corpus['lemma'] = preclean_corpus.index.str.lower()\n",
    "# remove any nan\n",
    "preclean_corpus = preclean_corpus.drop('nan')\n",
    "\n",
    "# isolate lemma columns and TF values to a new DataFrame\n",
    "d = {'lemma': preclean_corpus.iloc[:,1].values.tolist(), 'TF':preclean_corpus.iloc[:,0].values.tolist()}\n",
    "clean_corpus = pd.DataFrame(data = d)\n",
    "clean_corpus\n",
    "# save the processed frequency file\n",
    "clean_corpus.to_pickle('clean_corpus.pkl')"
   ]
  },
