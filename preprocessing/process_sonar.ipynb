   "source": [
    "This code takes in the frequency file of the SoNAR500 corpus from the \"Instituut van de Nederlandse Taal\"\n",
    "It preprocesses the file into a Term Frequency file, which can be used to score a given text on informativeness."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import pandas as pd\n",
    "import re\n",
    "import spacy\n",
    "# sets spacey parameters to Dutch\n",
    "nlp = spacy.load('nl_core_news_lg')\n",
    "import pickle\n",
    "import string\n",
    "import unidecode"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Load Corpus frequency file in as Pandas Dataframe\n",
    "corp_file = "SONAR500.lemmafreqlist.1-gram.total.tsv"\n",
    "df_corpus = pd.read_csv(corp_file, sep='\\t', names = ['lemma','abs_freq', 'acc_freq','acc_freq_perc' ])\n",
    "df_corpus\n"
   ]
  },
  {
   "cell_type": "markdown",
   "metadata": {},
   "source": [
    "Preprocess Frequency File"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 10,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Modify entries\n",
    "\n",
    "# remove accents from entries (e.g. \"√§\" --> \"a\")\n",
    "def remove_accent(x):\n",
    "    x = unidecode.unidecode(x)\n",
    "    return x\n",
    "\n",
    "# remove any entries that end with '.' (e.g. \"jaar.\"\" --> \"jaar\")\n",
    "def remove_period(x):\n",
    "    # ignore empty entries. Are removed later.\n",
    "    if x == '':\n",
    "        pass\n",
    "    # check if the last character is a \".\" and remove it\n",
    "    elif x[-1] == '.':\n",
    "        x = x[:-1]\n",
    "\n",
    "    return x\n",
    "\n",
    "\n",
    "# Modify entries\n",
    "df_words = df_corpus['lemma']\n",
    "df_words = df_words.apply(str).apply(remove_period)\n",
    "df_words = df_words.apply(str).apply(remove_accent)\n",
    "\n",
    "# replace entries with modified entries\n",
    "df_corpus['lemma'] = df_words"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 11,
   "metadata": {},
   "outputs": [],
   "source": [
    "## Remove all non-english words from corpus\n",
    "\n",
    "# Check if word is empty or containing non-ascii characters (e.g. emoticons or foreign characters)\n",
    "def isEnglish(s):\n",
    "    if s == '':\n",
    "        return False\n",
    "    try:\n",
    "        # Error if word contains non-ascii characters\n",
    "        # note rejects words with accents as well\n",
    "        s.encode('ascii')\n",
    "    except UnicodeEncodeError:\n",
    "        return False\n",
    "    else:\n",
    "        return True\n",
    "# Filter entries based on same letter sequences \n",
    "def find_tri(s, ma, da, ta):\n",
    "    # filter single letter entries (e.g. \"a\")\n",
    "    for mono in ma:\n",
    "        if mono == s:\n",
    "\n",
    "            return False\n",
    "    # Filter entries with three same-letter sequences (e.g. \"aaa\")\n",
    "    for tri in ta:\n",
    "        # Check if entry contains three same-letter sequences\n",
    "        if tri in s:\n",
    "            return False\n",
    "    # Check if entry is a three same-letter sequence\n",
    "    if any(substring == s for substring in ta):\n",
    "        return False\n",
    "    # Check if entry is a two same-letter sequence\n",
    "    elif any(substring == s for substring in da):\n",
    "        # print(s)\n",
    "        return False\n",
    "    else:\n",
    "        return True\n",
    "    \n",
    "# create all letters and letter repetitions (e.g. aa-zz, and aaa-zzz)\n",
    "mo_alphabet = [x for x in list(string.ascii_lowercase)]\n",
    "di_alphabet = [x*2 for x in list(string.ascii_lowercase)]\n",
    "tri_alphabet = [x*3 for x in list(string.ascii_lowercase)]        \n",
    "\n",
    "# Find all entries with letter repetitions\n",
    "tria_check_bool = list(df_words.apply(str).apply(find_tri,args=(mo_alphabet, di_alphabet, tri_alphabet)))\n",
    "tria_index = np.where(tria_check_bool)[0]\n",
    "\n",
    "# find all strings with only letters, also finds foreign characters\n",
    "ascii_check_bool = list(df_words.apply(str).apply(isEnglish))\n",
    "# find which entries are alphanumeric\n",
    "alpha_check_bool = list(df_words.str.isalpha())\n",
    "# Get indexes of word that are only alphanumeric, only contain ascii characters, and are not letter repetitions \n",
    "full_check_bool = list(np.logical_and(ascii_check_bool,alpha_check_bool))\n",
    "full_check_bool = list(np.logical_and(full_check_bool,tria_check_bool))\n",
    "full_index = np.where(full_check_bool)[0]\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 12,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Filter all excluded words from corpus\n",
    "df_filtered = df_corpus.iloc[full_index]\n",
    "\n",
    "## Uncomment to filter included words --> gives a list of which words are filtered\n",
    "# full_bool_c = full_check_bool\n",
