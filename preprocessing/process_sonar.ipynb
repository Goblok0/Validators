    "# Preprocess the respondent answers via the same process as the frequency file\n",
    "\n",
    "TFX_df = pd.read_pickle('D:/VU/Validators/code/TFX_df.pkl')\n",
    "\n",
    "def remove_accent(x):\n",
    "    x = unidecode.unidecode(x)\n",
    "    return x\n",
    "\n",
    "# Process each word in the answer and assign a term frequency score\n",
    "def score_informativeness(answer):\n",
    "\n",
    "    score_list = []\n",
    "    zero_words = []\n",
    "\n",
    "    # Strip each word\n",
    "    strip_answer = re.sub(r'[^\\w\\s]', '', answer).lower().split(' ')\n",
    "    pipe_answer = nlp.pipe(strip_answer)\n",
    "\n",
    "    # Lemmatize and score each word in the answer\n",
    "    lemma_answer = []\n",
    "    score_words = []\n",
    "    for doc in pipe_answer:\n",
    "        # Go through each word in the answer\n",
    "        for token in doc:\n",
    "            # Lemmatize the token (i.e. the word in the answer)\n",
    "            lem_word = token.lemma_\n",
    "            # remove accents\n",
    "            lemma_answer.append(remove_accent(lem_word))\n",
    "            try:\n",
    "                score_words.append(list(TFX_df.loc[TFX_df['lemma']==lem_word, 'TFX'])[0])\n",
    "            # Keep track of words without any score assignment\n",
    "            except: \n",
    "                zero_words.append(lem_word)\n",
    "                score_words.append(0)\n",
    "\n",
    "    # round informativeness score to 2 decimals\n",
    "    score_list.append(list(map(lambda x: round(x, 2), score_words)))\n",
    "    return score_list[0], zero_words"
   ]
